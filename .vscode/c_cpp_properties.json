{
  "env": {
    "presetName": "x64-linux-gcc-static-debug"
  },
  "configurations": [
    {
      "name": "build",
      "compilerPath": "/opt/rh/gcc-toolset-13/root/usr/bin/gcc",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/tests",
        "/usr/include"
      ],
      "cStandard": "gnu23",
      "cppStandard": "c++20",
      "configurationProvider": "ms-vscode.cmake-tools",
      "compileCommands": "${workspaceFolder}/build/${presetName}/compile_commands.json",
      "browse": {
        "path": [
          "${workspaceFolder}",
          "/usr/include"
        ],
        "limitSymbolsToIncludedHeaders": false
      }
    }
  ],
  "version": 4,
  "enableConfigurationSquiggles": true
}
