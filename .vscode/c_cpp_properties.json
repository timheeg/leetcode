{
  "env": {
    "project_include_path": [
      "${workspaceFolder}/src/0xx/**",
      "${workspaceFolder}/src/1xx/**",
      "${workspaceFolder}/src/2xx/**",
      "${workspaceFolder}/src/3xx/**",
      "${workspaceFolder}/tests/0xx/**",
      "${workspaceFolder}/tests/1xx/**",
      "${workspaceFolder}/tests/2xx/**",
      "${workspaceFolder}/tests/3xx/**",
      "/usr/include"
    ],
    "browse_path": [
      "${workspaceFolder}",
      "/usr/include"
    ]
  },
  "configurations": [
    {
      "name": "x64-linux-gcc-static-debug",
      "compilerPath": "/opt/rh/gcc-toolset-13/root/usr/bin/g++",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${project_include_path}"
      ],
      "cStandard": "gnu23",
      "cppStandard": "gnu++23",
      "configurationProvider": "ms-vscode.cmake-tools",
      "compileCommands": "${workspaceFolder}/build/x64-linux-gcc-static-debug/compile_commands.json",
      "browse": {
        "path": [
          "${browse_path}"
        ],
        "limitSymbolsToIncludedHeaders": false
      }
    },
    {
      "name": "x64-linux-gcc-static-release",
      "compilerPath": "/opt/rh/gcc-toolset-13/root/usr/bin/g++",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${project_include_path}"
      ],
      "cStandard": "gnu23",
      "cppStandard": "gnu++23",
      "configurationProvider": "ms-vscode.cmake-tools",
      "compileCommands": "${workspaceFolder}/build/x64-linux-gcc-static-release/compile_commands.json",
      "browse": {
        "path": [
          "${browse_path}"
        ],
        "limitSymbolsToIncludedHeaders": false
      }
    }
  ],
  "version": 4,
  "enableConfigurationSquiggles": true
}
